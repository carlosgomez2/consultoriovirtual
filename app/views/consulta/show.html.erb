<div class="page-header">
  <%= link_to consulta_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_consultum_path(@consultum), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show consultum</h1>
</div>

<dl class="dl-horizontal">
  <dt>Motivo consulta:</dt>
  <dd><%= @consultum.Motivo_Consulta %></dd>

  <dt>Diagnostico:</dt>
  <dd><%= @consultum.Diagnostico %></dd>

  <dt>Tx:</dt>
  <dd><%= @consultum.TX %></dd>

  <dt>Labs:</dt>
  <dd><%= @consultum.Labs %></dd>

  <dt>Observaciones:</dt>
  <dd><%= @consultum.Observaciones %></dd>

</dl>
