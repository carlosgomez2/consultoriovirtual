<%= simple_form_for(@consultum) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :Motivo_Consulta %>
    <%= f.input :Diagnostico %>
    <%= f.input :TX %>
    <%= f.input :Labs %>
    <%= f.input :Observaciones %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
